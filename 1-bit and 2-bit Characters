# 1-bit and 2-bit Characters

class Solution:
    def isOneBitCharacter(self, bits):
        i = 0
        n = len(bits)
        
        while i < n - 1:       # stop before last index
            if bits[i] == 1:
                i += 2        # 2-bit character
            else:
                i += 1        # 1-bit character
        
        return i == n - 1
