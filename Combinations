#  Combinations

class Solution:
    def combine(self, n: int, k: int):
        res = []
        
        def backtrack(start, path):
            # If combination complete
            if len(path) == k:
                res.append(path[:])
                return
            
            # Try all possible next numbers
            for i in range(start, n + 1):
                path.append(i)
                backtrack(i + 1, path)
                path.pop()  # backtrack
        
        backtrack(1, [])
        return res
